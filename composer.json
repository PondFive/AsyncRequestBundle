{
    "name": "pond5/async-request-bundle",
    "description": "This bundle allows sending requests to a Symfony Messenger transport to be handled later by a consumer",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Pond5\\AsyncRequestBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "psr/log": "^1.1",
        "symfony/config": "^5.2|^6.0",
        "symfony/dependency-injection": "^5.2|^6.0",
        "symfony/event-dispatcher": "^5.2|^6.0",
        "symfony/http-foundation": "^5.2|^6.0",
        "symfony/http-kernel": "^5.2|^6.0",
        "symfony/messenger": "^5.2|^6.0"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan": "^0.12.85",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin"
    }
}
